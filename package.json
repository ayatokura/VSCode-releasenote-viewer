{
    "name": "releasenote-viewer",
    "displayName": "VSCode Japanese Release Notes Viewer",
    "description": "",
    "version": "0.0.1",
    "publisher": "ayatokura",
    "engines": {
        "vscode": "^1.12.0"
    },
    "categories": [
        "Other"
    ],
    "icon": "images/claudia_hanko_1494764677009.png",
    "activationEvents": [
        "onCommand:extension.relnoteja"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
        {
            "command": "extension.relnoteja",
            "title": "JP-VSCode-Docs: Visual Studio Code 日本語リリースノート"
        }
        ],
        "configuration": {
        "type": "object",
        "title": "vscode-relnote-ja",
        "order": 20,
        "properties": {
            "vscode-relnote-ja.showPreviewToSide": {
            "type": "boolean",
            "default": false
            }
        }
        }
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "dependencies": {
        "request": "^2.81.0"
    }
}